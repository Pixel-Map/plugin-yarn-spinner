{
    "Commands": [
        {
            "YarnName": "add_gold",
            "DefinitionName": "add_gold",
            "Documentation": "Adds X amount of gold to the player",
            "Signature": "add_gold amount",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "amount",
                    "Type": "Number",
                    "Documentation": "Any number, including negative, but then you should probably just use remove_gold",
                    "IsParamsArray": false
                }
            ]
        },
        {
            "YarnName": "add_item",
            "DefinitionName": "add_item",
            "Documentation": "Gives the player x number of a given item.",
            "Signature": "add_item itemName quantity",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "itemName",
                    "Type": "String",
                    "Documentation": "Name of the item to give",
                    "IsParamsArray": false
                },
                {
                    "Name": "quantity",
                    "Type": "Number",
                    "Documentation": "quantity",
                    "IsParamsArray": false,
                    "DefaultValue": "1"
                }
            ]
        },
        {
            "YarnName": "fade_in",
            "DefinitionName": "fade_in",
            "Documentation": "fade_in to normal (from black screen or tint)",
            "Signature": "fade_in numberOfFrames",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "numberOfFrames",
                    "Type": "Number",
                    "Documentation": "Number of frames to fade in (default of 24)",
                    "IsParamsArray": false,
                    "DefaultValue": "24"
                }
            ]
        },
        {
            "YarnName": "fade_out",
            "DefinitionName": "fade_out",
            "Documentation": "fade_out to black screen (or tinted)",
            "Signature": "fade_out duration red green blue grey alpha",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "duration",
                    "Type": "Number",
                    "Documentation": "Number of frames to fade out (default of 24)",
                    "IsParamsArray": false,
                    "DefaultValue": "24"
                },
                {
                    "Name": "red",
                    "Type": "Number",
                    "Documentation": "Amount of red (RGB value, default of 0)",
                    "IsParamsArray": false,
                    "DefaultValue": "0"
                },
                {
                    "Name": "green",
                    "Type": "Number",
                    "Documentation": "Amount of green (RGB value, default of 0)",
                    "IsParamsArray": false,
                    "DefaultValue": "0"
                },
                {
                    "Name": "blue",
                    "Type": "Number",
                    "Documentation": "Amount of blue (RGB value, default of 0)",
                    "IsParamsArray": false,
                    "DefaultValue": "0"
                },
                {
                    "Name": "grey",
                    "Type": "Number",
                    "Documentation": "Amount of grey (RGB value, default of 0)",
                    "IsParamsArray": false,
                    "DefaultValue": "0"
                },
                {
                    "Name": "alpha",
                    "Type": "float",
                    "Documentation": "Amount of alpha (transparency value, default of 1, decimal between 0 and 1)",
                    "IsParamsArray": false,
                    "DefaultValue": "1"
                }
            ]
        },
        {
            "YarnName": "flash_screen",
            "DefinitionName": "flash_screen",
            "Documentation": "flash_screen (optionally tinted)",
            "Signature": "flash_screen duration red green blue intensity",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "duration",
                    "Type": "Number",
                    "Documentation": "Number of frames for flash (default of 8)",
                    "IsParamsArray": false,
                    "DefaultValue": "8"
                },
                {
                    "Name": "red",
                    "Type": "Number",
                    "Documentation": "Amount of red (RGB value, default of 0)",
                    "IsParamsArray": false,
                    "DefaultValue": "0"
                },
                {
                    "Name": "green",
                    "Type": "Number",
                    "Documentation": "Amount of green (RGB value, default of 0)",
                    "IsParamsArray": false,
                    "DefaultValue": "0"
                },
                {
                    "Name": "blue",
                    "Type": "Number",
                    "Documentation": "Amount of blue (RGB value, default of 0)",
                    "IsParamsArray": false,
                    "DefaultValue": "0"
                },
                {
                    "Name": "intensity",
                    "Type": "Number",
                    "Documentation": "Amount of intensity (Default and max of 255))",
                    "IsParamsArray": false,
                    "DefaultValue": "255"
                }
            ]
        },
        {
            "YarnName": "hide_entity",
            "DefinitionName": "hide_entity",
            "Documentation": "Hides the entity (via passed in name).  If no entity specified, hides the calling entity",
            "Signature": "hide_entity entityName",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "entityName",
                    "Type": "String",
                    "Documentation": "If unspecified, assumes you meant the calling entity",
                    "IsParamsArray": false,
                    "DefaultValue": "this"
                }
            ]
        },
        {
            "YarnName": "move_event",
            "DefinitionName": "move_event",
            "Documentation": "Moves an Event",
            "Signature": "move_event direction distance speed eventName",
            "Language": "text",
            "Parameters": [
                
                {
                    "Name": "direction",
                    "Type": "String",
                    "Documentation": "up down left right",
                    "IsParamsArray": false
                },
                {
                    "Name": "distance",
                    "Type": "number",
                    "Documentation": "Distance to move",
                    "IsParamsArray": false
                },
                {
                    "Name": "speed",
                    "Type": "number",
                    "Documentation": "Speed to move, default 0.25",
                    "IsParamsArray": false,
                    "DefaultValue": "0.25"                    
                },
                {
                    "Name": "eventName",
                    "Type": "String",
                    "Documentation": "If called without specifying eventName, assumes calling entity",
                    "IsParamsArray": false,
                    "DefaultValue": "this"
                }
            ]
        },
        {
            "YarnName": "play_music",
            "DefinitionName": "play_music",
            "Documentation": "Play Music (use default unless specified)",
            "Signature": "play_music musicName",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "musicName",
                    "Type": "String",
                    "Documentation": "Name of music, defaults to what WAS playing",
                    "IsParamsArray": false
                }
            ]
        },
        {
            "YarnName": "play_sound",
            "DefinitionName": "play_sound",
            "Documentation": "Plays a sound effect at X volume (0-100).",
            "Signature": "play_sound name volume",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "name",
                    "Type": "String",
                    "Documentation": "The name of the sound effect, without the extension",
                    "IsParamsArray": false
                },
                {
                    "Name": "volume",
                    "Type": "Number",
                    "Documentation": "The volume to play the SFX, from 0 to 100",
                    "IsParamsArray": false
                }
            ]
        },
        {
            "YarnName": "remove_gold",
            "DefinitionName": "remove_gold",
            "Documentation": "Removes X amount of gold from the player",
            "Signature": "remove_gold amount",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "amount",
                    "Type": "Number",
                    "Documentation": "Any number, including negative, but then you should just use AddGold",
                    "IsParamsArray": false
                }
            ]
        },
        {
            "YarnName": "remove_item",
            "DefinitionName": "remove_item",
            "Documentation": "Removes x number of a given item from player.",
            "Signature": "remove_item itemName quantity",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "itemName",
                    "Type": "String",
                    "Documentation": "ID of the item to remove",
                    "IsParamsArray": false
                },
                {
                    "Name": "quantity",
                    "Type": "Number",
                    "Documentation": "quantity",
                    "IsParamsArray": false
                }
            ]
        },
        {
            "YarnName": "set_background",
            "DefinitionName": "set_background",
            "Documentation": "Set the message box background transparency level.",
            "Signature": "set_background bgType",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "bgType",
                    "Type": "Number",
                    "Documentation": "You can specify 0, 1, or 2.",
                    "IsParamsArray": false
                }
            ]
        },
        {
            "YarnName": "set_facing",
            "DefinitionName": "set_facing",
            "Documentation": "Changes the direction the provided entity name is facing, or if just a direction is passed, assumes you meant the calling entity.",
            "Signature": "set_facing direction entityName",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "direction",
                    "Type": "String",
                    "Documentation": "up down left right",
                    "IsParamsArray": false
                },
                {
                    "Name": "entityName",
                    "Type": "String",
                    "Documentation": "You can optionally NOT provide this parameter, and we will assume you meant the calling entity",
                    "IsParamsArray": false,
                    "DefaultValue": "this"
                }
                
            ]
        },
        {
            "YarnName": "show_entity",
            "DefinitionName": "show_entity",
            "Documentation": "Shows the entity (via passed in name).  If no entity specified, shows the calling entity",
            "Signature": "show_entity entityName",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "entityName",
                    "Type": "String",
                    "Documentation": "If unspecified, assumes you meant the calling entity",
                    "IsParamsArray": false,
                    "DefaultValue": "this"
                }
            ]
        },
        {
            "YarnName": "stop_music",
            "DefinitionName": "stop_music",
            "Documentation": "Stop music with optional fadeout",
            "Signature": "stop_music interval",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "interval",
                    "Type": "Number",
                    "Documentation": "Length of time to fade in seconds",
                    "IsParamsArray": false
                }
            ]
        },
        {
            "YarnName": "wait",
            "DefinitionName": "wait",
            "Documentation": "Wait the given number of ms",
            "Signature": "Wait time",
            "Language": "text",
            "Parameters": [
                {
                    "Name": "time",
                    "Type": "Number",
                    "Documentation": "Time in milliseconds",
                    "IsParamsArray": false
                }
            ]
        }
    ],
